class Solve {
    int[] findTwoElement(int arr[], int n) {
        // code here
        //  int arr[]={1, 3, 3};
      
       int fin[]= new int[2];
       for(int value:arr)
       {
           int absvalue=Math.abs(value);
           if(arr[absvalue-1]<0) fin[0]=absvalue;
           else
           arr[absvalue-1]*=-1;
       }
       for(int i=0;i<arr.length;i++)
       {
           if(arr[i]>0) 
           {fin[1]=i+1;
           
           }
       }
       
        return fin;
    }
}
